    <script>
        function signupGet(){
            const form = document.forms['resister'];
            let hobbyarr = [];
            for(let i=0; i < 3; i++){
                if(form.hobby[i].checked==true){
                    hobbyarr.push(form.hobby[i].value);
                }
            }

            const data = {
                name: form.name.value,
                gender: form.gender.value,
                birth: `${form.year.value}-${form.mounth.value}-${form.day.value}`,
                hobby: hobbyarr,
            }
            axios({
                method: 'GET',
                url:'/axios',
                params: data,
            }).then((res) => {
                console.log(res);
         
            })
        }
    </script>
